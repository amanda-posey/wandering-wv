<div class="container-fluid">
<div class="row main">
<div class="container col-8">
<%data.forEach(eachData => {%>
   
<div class="card mb-3">
    <img src="<%= eachData.dataValues.image %> " class="card-img-top" alt=" <%= eachData.dataValues.title %>  ">
    <h5 class="card-title"><a href="/places/<%= eachData.dataValues.id %>"><%= eachData.dataValues.title %></a></h5>
    <p class="card-text"> <%= eachData.dataValues.content %> </p>
    <p class="card-text"><small class="text-muted">Last updated: <%= eachData.dataValues.updatedAt %> </small></p>
    <div class="card-body">
      
    </div>
  </div>
  <%})%>
</div>

<div class="container col-3">
  <div class="list-group">
      <a href="#" class="list-group-item list-group-item-action active">
        Explore By County
      </a>
      <%data.forEach(c => {%>
        <a href="/county/<%= c.dataValues.countyName %> " class="list-group-item list-group-item-action"><%= c.dataValues.countyName %> </a>
      <%})%> 
    </div>
  </div>
</div>
</div>

</div>
</div>